{
  "data_collection": {
    "snapshot_frequency_seconds": 300,
    "max_current_files": 100,
    "max_hourly_files": 168,
    "max_daily_files": 30,
    "compression_enabled": true,
    "auto_cleanup_enabled": true
  },
  "retention_policies": {
    "current_data_hours": 24,
    "hourly_data_days": 7,
    "daily_data_days": 30,
    "monthly_data_days": 365
  },
  "storage_limits": {
    "max_total_size_gb": 10,
    "max_files_per_directory": 1000,
    "cleanup_threshold_percent": 80
  },
  "logging": {
    "log_level": "INFO",
    "log_rotation_days": 7,
    "max_log_size_mb": 100
  }
}
